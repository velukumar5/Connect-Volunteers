<body>

  
    <div class="main-wrapper">
        <div class="header">
            <h1>Volunteer Register</h1>
			</div>
        </div>
            </div>
        </div>
        <div class="page-wrapper">
            <div class="content">
                <div class="row">
                    <div class="col-lg-8 offset-lg-2">
                        <h4 class="page-title">Volunteer Registration</h4>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-8 offset-lg-2">
                        <form method="POST">
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label> Volunteer Name <span class="text-danger">*</span></label>
                                        <input class="form-control" type="text" name="name">
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label>Email</label>
                                        <input class="form-control" type="email" name="email">
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label> Contact <span class="text-danger">*</span></label>
                                        <input class="form-control" type="text" name="contact" >
                                    </div>
                                </div>

                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label> Aadhar No <span class="text-danger">*</span></label>
                                        <input class="form-control" type="text" name="salary" >
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label>Address <span class="text-danger">*</span></label>
                                        <input class="form-control" type="text" name="date">
                                    </div>
                                </div>
                                
                                
                               
                               
                               
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label>Experience/Skill/Role</label>
                                        <select class="" name="role">
                                            <option value="Student">Student</option>
                                            <option value="NCC/NSS">NCC/NSS</option>
                                            <option value="Doctor">Doctor</option>
                                            <option value="Driver">Drive</option>
                                            <option value="Swimming">Swimming</option>
                                            <option value="other">other</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                          
                            <div class="m-t-20 text-center">
                                <button onclick="myFunction()" class="btn btn-primary submit-btn">Register Volunteer</button>

                            </div>

                            
                                
                        </form>
                    </div>
                </div>
            </div>
			        </div>
    </div>
    <div class="sidebar-overlay" data-reff=""></div>
